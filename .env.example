# Astro runtime uses these to connect to Sanity at build/runtime
SANITY_PROJECT_ID=your-sanity-project-id
SANITY_DATASET=production
SANITY_API_VERSION=2023-05-26
SANITY_USE_CDN=false

# Optional tokens for authenticated reads/writes
# SANITY_READ_TOKEN=your-read-token
# SANITY_WRITE_TOKEN=your-write-token

# Sanity Studio (./sanity) picks up the same values but can be overridden
SANITY_STUDIO_PROJECT_ID=your-sanity-project-id
SANITY_STUDIO_DATASET=production
